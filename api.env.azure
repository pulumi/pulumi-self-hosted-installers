# Azure-specific env vars

# Azure storage account name
AZURE_STORAGE_ACCOUNT=

# Azure Managed System Identity's (MSI) client ID for both
# Azure Storage and Azure KeyVault.
# AZURE_CLIENT_ID=

# If not using Azure MSI, specify the rest of the AAD application
# client credentials.
# AZURE_CLIENT_SECRET=
# AZURE_TENANT_ID=
# AZURE_SUBSCRIPTION_ID=

# IMPORTANT: If not using MSI, specify the storage account key.
# AZURE_STORAGE_KEY=
